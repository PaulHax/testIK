{
  "name": "three-actor",
  "description": "Character behaviors for three.js module",
  "version": "0.0.1",
  "license": "MIT",
  "scripts": {
    "build": "rollup -c rollup.config.js",
    "dev": "rollup -w -c rollup.dev.config.js",
    "predeploy": "npm run build && shx mkdir -p .ghpages && shx cp -r examples/index.html examples/assets build/build.module.js .ghpages && rollup -c rollup.examples.config.js",
    "trydeploy": "npm run predeploy && sirv start .ghpages",
    "deploy": "npm run predeploy && gh-pages -d .ghpages && shx rm -rf .ghpages"
  },
  "peerDependencies": {
    
  },
  "devDependencies": {
    "@babel/core": "^7.8.3",
    "@rollup/plugin-node-resolve": "^7.0.0",
    "rollup": "^1.29.0",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-cleanup": "^1.0.1",
    "rollup-plugin-commonjs": "^8.2.1",
    "rollup-plugin-livereload": "^1.0.4",
    "rollup-plugin-serve": "^1.0.1",
    "shx": "^0.2.2",
    "gh-pages": "^2.1.1",
    "sirv-cli": "^0.4.4",
    "three": "0.112.0"
  },
  "dependencies": {}
}
